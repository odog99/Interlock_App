        <activity
            android:name="com.oep.interlock_app.CleaningSealing"
            android:label="@string/cleaning_sealing_title" />
        <activity
            android:name="com.oep.interlock_app.CleaningSealing2"
            android:label="@string/cleaning_sealing_title" />
        <activity
            android:name="com.oep.interlock_app.CleaningSealingEstimation"
            android:configChanges="orientation|screenSize"
            android:label="@string/cleaning_sealing_title" />
        <activity android:name="com.oep.interlock_app.EstimationPage"></activity>
    </application>

</manifest>